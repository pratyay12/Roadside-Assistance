<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roadsideassistant">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".ForgotActivity"></activity>
        <activity android:name=".DataRetrieved" />
        <activity android:name=".Emergancy" />
        <activity android:name=".AddRefreshmentServices" />
        <activity android:name=".AddTwoWheeler" />
        <activity android:name=".AddHospitalServices" />
        <activity android:name=".AddFourWheelerServices" />
        <activity android:name=".HomeAdmin" />
        <activity android:name=".Register" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>